# Description:
# URL:
# Maintainer:
# Packager:
# Depends on: libwnck

name=gnome-python-desktop
version=2.18.0
release=1
source=(http://download.gnome.org/desktop/2.18/2.18.0/sources/$name-$version.tar.bz2)

build() {
	cd $name-$version
	./configure --prefix=/usr
	make
	make DESTDIR=$PKG install
	rm -rf $PKG/usr/share/gtk-doc
}
